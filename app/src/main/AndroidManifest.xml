<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.fuskerr63.androidlesson">

    <uses-permission android:name="android.permission.READ_CONTACTS"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="com.gmail.fuskerr63.service.ContactService"
            android:enabled="true">
        </service>

        <receiver
            android:name="com.gmail.fuskerr63.receiver.ContactReceiver"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.gmail.fuskerr63.action.notification"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>